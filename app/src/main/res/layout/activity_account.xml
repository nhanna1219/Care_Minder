        tools:layout_editor_absoluteX="62dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Height"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView12"
        tools:layout_editor_absoluteX="244dp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Height"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/dividerView4"
        app:layout_constraintTop_toBottomOf="@+id/dividerView5"
        tools:layout_editor_absoluteX="70dp" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="53dp"
        android:layout_height="31dp"
        android:text="Weight"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/dividerView6"
        app:layout_constraintTop_toBottomOf="@+id/dividerView4"
        tools:layout_editor_absoluteX="69dp" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="cm"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/dividerView4"
        app:layout_constraintTop_toTopOf="@+id/dividerView5"
        tools:layout_editor_absoluteX="312dp" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="°C"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="312dp"
        tools:layout_editor_absoluteY="553dp" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="kg"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="310dp"
        tools:layout_editor_absoluteY="476dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/rectangle" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/rectangle_with_text"
        android:gravity="center"
        android:text="Male"
        android:textColor="@color/black"
        app:layout_constraintEnd_toStartOf="@+id/textView10"
        app:layout_constraintHorizontal_bias="0.462"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="186dp" />

