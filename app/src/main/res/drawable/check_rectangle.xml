<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/shadow"/>
            <stroke android:color="#DCD9D9" android:width="2dp" />
            <corners android:radius="15dp"/>
        </shape>
    </item>
    <item android:bottom="5dp">
        <shape android:shape="rectangle">
            <size android:height="130dp" android:width="130dp"/>
            <solid android:color="#DAD1DA" />
            <stroke android:color="#FCEAE5EA" android:width="2dp"/>
            <corners android:radius="15dp" />
        </shape>
    </item>

    <item android:drawable="@drawable/ic_check"
        android:height="24dp"
        android:width="24dp"
        android:left="90dp"
        android:top="10dp">
    </item>

</layer-list>

